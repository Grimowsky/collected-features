{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@shared-ui/*": ["libs/shared-ui/src/*"]
    },
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "dist"]
}
